        android:roundIcon="@mipmap/s"
        android:supportsRtl="true"
        android:theme="@style/Theme.JuegoMinecraft"
        tools:targetApi="31">
        <activity
            android:name=".JuegoActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SQLiteActivity" android:exported="true"></activity>
        <activity android:name=".PuntuacioFinalActivity" android:exported="true"></activity>
    </application>

</manifest>